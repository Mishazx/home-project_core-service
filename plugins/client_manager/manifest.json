{
  "name": "client_manager",
  "version": "0.1.0",
  "description": "Remote Client Manager as a plugin (docker delivery)",
  "type": "docker",
  "artifact": {
    "image": "mishazx/client_manager:0.1.0",
    "pull_policy": "IfNotPresent"
  },
  "entrypoint": null,
  "install": {
    "strategy": "docker",
    "run": {
      "detach": true,
      "args": ["--restart=unless-stopped"]
    }
  }
}
